{
  "name": "fn-ts",
  "version": "1.0.0",
  "description": "firenotifications",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "dev:ts-node": "ts-node --transpile-only -r tsconfig-paths/register src/server.ts",
    "dev:nodemon": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "migration:create": "tsx src/scripts/create-migration.ts",
    "migration:status": "tsx src/scripts/migrate-status.ts",
    "migrate": "tsx src/scripts/migrate.ts up",
    "migrate:down": "tsx src/scripts/migrate.ts down",
    "migrate:fresh": "npm run migrate:down && npm run migrate",
    "migrate:to": "tsx src/scripts/migrate-to.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "workers": "tsx src/workers/worker.bootstrap.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "socket.io"
  ],
  "author": "0xsj",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^30.0.3",
    "@swc/core": "^1.12.9",
    "@swc/helpers": "^0.5.17",
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^30.0.3",
    "kysely-codegen": "^0.18.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "tsyringe": "^4.10.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "@bull-board/express": "^6.10.1",
    "@types/redis": "^4.0.10",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.56.0",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "generic-pool": "^3.9.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.2",
    "mysql2": "^3.14.1",
    "nodemailer": "^7.0.4",
    "otplib": "^12.0.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "pino-roll": "^3.1.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^5.5.6",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "swagger-ui-themes": "^3.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  }
}
